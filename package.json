{
  "name": "openInAndroidStudio",
  "displayName": "open-in-android-studio",
  "description": "在 Android Studio 中打开当前文件或项目",
  "version": "0.0.3",
  "publisher": "wangqiankun",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangqiankun/openInAndroidStudio.git"
  },
  "engines": {
    "vscode": "*"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "android",
    "android-studio",
    "open-file"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#C2E7FF",
    "theme": "light"
  },
  "activationEvents": [
    "onCommand:openInAndroidStudio.openFile",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "openInAndroidStudio.openFile",
        "title": "在 Android Studio 中打开"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "openInAndroidStudio.openFile",
          "group": "1_modification",
          "when": "resourceExtname"
        }
      ],
      "editor/context": [
        {
          "command": "openInAndroidStudio.openFile",
          "group": "1_modification",
          "when": "resourceExtname"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "*",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
